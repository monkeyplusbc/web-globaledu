<!-- Netlify cms redirect -->
<% if(it?.page?.name==='home'){%>
<script>
      if (location.hash && (location.hash.includes('invite_token') || location.hash.includes('recovery') || location.hash.includes('access_token'))) {
        // Guard to redirect
        location.href = `/cms/v2/${location.hash}`;
      }
</script>
<% }%>